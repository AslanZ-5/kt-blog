{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["react-app", "airbnb", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": "off"
  }
}
